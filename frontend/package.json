{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "axios-base-url": "^0.2.0",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.2.0",
    "core-js": "^3.6.4",
    "jquery": "^1.12.4",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vue-select": "^3.9.5",
    "vue-simple-search-dropdown": "^1.0.1",
    "vue-star-rating": "^1.6.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.2.3",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/test-utils": "^1.0.0-beta.33",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^25.3.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ]
  },
  "description": "```\r npm install\r ```",
  "main": "babel.config.js",
  "directories": {
    "test": "tests"
  },
  "author": "",
  "license": "ISC"
}
